on: [push]

jobs:
  build_job:
    runs-on: ubuntu-latest
    name: build pkg
    steps:
      - uses: actions/checkout@v3
      - name: build pkg
        id: build
        uses: ./.github/actions/build
        with:
          version: '0.${{ github.run_number }}.${{ github.run_attempt }}'
          target: 'update'
          