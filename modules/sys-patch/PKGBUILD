# Maintainer: FeiLong <feilongphone@gmail.com>
pkgname=("sys-patch")
pkgdesc="A script-like system module that patches fs, es, ldr and nifm on boot"
pkgver=1.5.8
pkgrel=1
source=(
        "fusee.zip::https://github.com/impeeza/sys-patch/releases/download/v${pkgver}/sys-patch-${pkgver}-v5.zip"
)
arch=("any")

package() {
    pushd ${srcdir}
      find -type f -exec install -Dm 755 "{}" "${pkgdir}/{}" \;
    popd
}
md5sums=('6135fd1f053ced280ac9a834b03cb494')
