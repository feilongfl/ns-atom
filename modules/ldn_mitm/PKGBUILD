pkgname=("ldn_mitm")
pkgdesc="atomspheer for Nintendo Switch"
pkgver=1.13.0
pkgrel=1
github=spacemeowx2/ldn_mitm
source=(
      "ldn_mitm.zip::https://github.com/${github}/releases/download/v${pkgver}/ldn_mitm_v${pkgver}.zip"
)
arch=("any")

package() {
	pushd ${srcdir}
      find -type f -exec install -Dm 755 "{}" "${pkgdir}/{}" \;
      popd
}
