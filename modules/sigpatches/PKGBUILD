pkgname=("sigpatches")
pkgdesc="atomspheer for Nintendo Switch"
pkgver=14.1.0-1.3.2
pkgrel=1
github=ITotalJustice/patches
source=(
        "fusee.zip::https://github.com/${github}/releases/download/${pkgver}/SigPatches.zip"
)
arch=("any")

package() {
    pushd ${srcdir}
      find -type f -exec install -Dm 755 "{}" "${pkgdir}/{}" \;
    popd
}
