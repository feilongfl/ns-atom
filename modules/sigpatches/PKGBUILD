pkgname=("sigpatches")
pkgdesc="atomspheer for Nintendo Switch"
pkgver=1.2.2
pkgrel=1
source=(
        "fusee.zip::https://github.com/ITotalJustice/patches/releases/download/14.1.0-1.3.2/SigPatches.zip"
)
arch=("any")

package() {
    pushd ${srcdir}
      find -type f -exec install -Dm 755 "{}" "${pkgdir}/{}" \;
    popd
}
md5sums=('6bb97820edf169052172430fe1b6dca4')
