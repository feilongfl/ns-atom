pkgname=("telsa-menu")
pkgdesc="atomspheer for Nintendo Switch"
pkgver=1.1.4
pkgrel=1
source=(
        "telsamenu.zip::https://github.com/WerWolv/Tesla-Menu/releases/download/v1.1.4/ovlmenu.zip"
)
arch=("any")

package() {
    pushd ${srcdir}
      find -type f -exec install -Dm 755 "{}" "${pkgdir}/{}" \;
    popd
}
