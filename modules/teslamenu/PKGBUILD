pkgname=("telsa-menu")
pkgdesc="atomspheer for Nintendo Switch"
pkgver=1.2.3
pkgrel=1
github=WerWolv/Tesla-Menu
source=(
        "telsamenu.zip::https://github.com/${github}/releases/download/v${pkgver}/ovlmenu.zip"
)
arch=("any")

package() {
    pushd ${srcdir}
      find -type f -exec install -Dm 755 "{}" "${pkgdir}/{}" \;
    popd
}
