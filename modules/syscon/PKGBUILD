pkgname=("syscon")
pkgdesc="atomspheer for Nintendo Switch"
pkgver=0.6.4
pkgrel=1
source=(
        "syscon.zip::https://github.com/cathery/sys-con/releases/download/v0.6.4/sys-con-0.6.4.zip"
)
arch=("any")

package() {
    pushd ${srcdir}
      find -type f -exec install -Dm 755 "{}" "${pkgdir}/{}" \;
    popd
}
